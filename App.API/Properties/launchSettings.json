{
  "profiles": {
    "Development": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "App.Api": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "user",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "user"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/user",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}